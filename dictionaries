        "# статистика по количеству слов в запросах\n",
        "queries = [\n",
        "    'смотреть сериалы онлайн',\n",
        "    'новости спорта',\n",
        "    'афиша кино',\n",
        "    'курс доллара',\n",
        "    'сериалы этим летом',\n",
        "    'курс по питону',\n",
        "    'сериалы про спорт',\n",
        "]\n",
        "blank = {}\n",
        "\n",
        "for el in queries:\n",
        "    words = el.split()\n",
        "    if len(words) in blank.keys():\n",
        "        blank[len(words)] += 1\n",
        "    else:\n",
        "        blank.update({len(words): 1})\n",
        "for key, value in blank.items():\n",
        "    percent = round((value / len(queries)) * 100, 2)\n",
        "    print(f'Поисковых запросов, содержащих {key} слов(а): {percent}%')"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
